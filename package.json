{
  "name": "util.keymaster",
  "version": "0.0.14",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Command line for generating SSH keys and self signed certs for a project.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.keymaster"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.keymaster/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "empty-dir": "^2.0.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.6",
    "util.join": "0.0.21",
    "util.rstrip": "0.0.40",
    "util.timestamp": "0.0.38",
    "util.toolbox": "0.0.68",
    "uuid": "^3.3.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.4",
    "@types/lodash": "^4.14.119",
    "@types/log4js": "2.3.5",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "@types/yargs": "^12.0.5",
    "coveralls": "^3.0.2",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-babel-jest": "^1.0.5",
    "util.fixture": "0.0.65",
    "util.pkgwrap": "^0.0.94"
  },
  "bin": {
    "keymaster": "./cli.js"
  },
  "keymaster": {
    "envTypes": [
      "development",
      "testing",
      "production"
    ],
    "sshKeys": [
      "buildmaster",
      "centos"
    ],
    "hash": {
      "size": 32,
      "combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
    }
  },
  "pkgwrap": {
    "cleanup": [
      "cli.js",
      "cli.d.ts",
      "index.js",
      "index.d.ts"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "notify": false,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__tests__/helpers"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      "^.+\\.ts?$": "typescript-babel-jest"
    },
    "verbose": false
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage --jest",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "test": "pkgwrap --testing --jest"
  }
}
