  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "empty-dir": "^0.2.1",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "util.join": "0.0.12",
    "util.pkgwrap": "0.0.59",
    "util.rstrip": "0.0.23",
    "util.timestamp": "0.0.31",
    "util.toolbox": "0.0.36",
    "uuid": "^3.1.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/fs-extra": "3.0.3",
    "@types/lodash": "^4.14.68",
    "@types/log4js": "0.0.32",
    "@types/node": "^8.0.7",
    "@types/uuid": "^3.0.0",
    "@types/yargs": "^8.0.0",
    "ava": "^0.20.0",
    "coveralls": "^2.13.1",
    "fs-extra": "^3.0.1",
    "nyc": "^11.0.3",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1",
    "util.fixture": "0.0.55"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "keymaster": "./cli.js"
  },
  "scripts": {
    "all": "npm install && npm run clean && npm run lint && npm run build && npm test && npm run report",
    "build": "pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "postinstall": "pkgwrap --postinstall",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  },
  "keymaster": {
    "envTypes": [
      "development",
      "testing",
      "production"
    ],
    "sshKeys": [
      "buildmaster",
      "centos"
    ],
    "hash": {
      "size": 32,
      "combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
    }
  }
}
