{
	"name": "util.keymaster",
	"version": "0.0.18",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Command line for generating SSH keys and self signed certs for a project.",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.keymaster"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.keymaster/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"bin": {
		"keymaster": "./cli.js"
	},
	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"cli.js",
		"index.js",
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"empty-dir": "^2.0.0",
		"lodash": "^4.17.11",
		"log4js": "^4.0.2",
		"util.join": "0.0.24",
		"util.rstrip": "0.0.47",
		"util.timestamp": "0.0.41",
		"util.toolbox": "0.0.73",
		"util.toolbox-node": "^0.0.2",
		"uuid": "^3.3.2",
		"yargs": "^13.2.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/plugin-transform-modules-umd": "^7.2.0",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.3.4",
		"@types/fs-extra": "5.0.5",
		"@types/jest": "^24.0.9",
		"@types/lodash": "^4.14.122",
		"@types/log4js": "2.3.5",
		"@types/node": "^11.10.5",
		"@types/uuid": "^3.4.4",
		"@types/yargs": "^12.0.9",
		"babel-minify": "^0.5.0",
		"babel-preset-minify": "^0.5.0",
		"coveralls": "^3.0.3",
		"fs-extra": "^7.0.1",
		"jest": "^24.3.1",
		"prettier": "^1.16.4",
		"tslint": "^5.13.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.3.3333",
		"util.fixture": "0.0.69",
		"util.pkgwrap": "^0.0.104"
	},
	"keymaster": {
		"envTypes": [
			"development",
			"testing",
			"production"
		],
		"sshKeys": [
			"buildmaster",
			"centos"
		],
		"hash": {
			"size": 32,
			"combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
		}
	},
	"pkgwrap": {
		"cleanup": [
			"cli.js",
			"cli.js.map",
			"cli.d.ts"
		]
	},
	"scripts": {
		"all": "yarn install && yarn build && yarn test",
		"build": "yarn clean && yarn lint && yarn prettier && yarn build:es && yarn build:umd && yarn build:cli",
		"build:es": "tsc -p . && minify index.js --out-file index.es.min.js",
		"build:umd": "babel index.js --out-file index.umd.min.js",
		"build:cli": "babel cli.js --out-file cli.js",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"lint": "pkgwrap --lint",
		"prepublishOnly": "yarn run all",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,css}\"",
		"test": "jest"
	}
}
