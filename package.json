{
  "name": "util.keymaster",
  "version": "0.0.12",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Command line for generating SSH keys and self signed certs for a project.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.keymaster"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.keymaster/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "empty-dir": "^1.0.0",
    "lodash": "^4.17.5",
    "log4js": "^2.5.3",
    "util.join": "0.0.18",
    "util.rstrip": "0.0.34",
    "util.timestamp": "0.0.36",
    "util.toolbox": "0.0.58",
    "uuid": "^3.2.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.2",
    "@types/lodash": "^4.14.107",
    "@types/log4js": "2.3.5",
    "@types/node": "^9.6.6",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^11.0.0",
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "fs-extra": "^5.0.0",
    "nyc": "^11.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "util.fixture": "0.0.62",
    "util.pkgwrap": "^0.0.91"
  },
  "bin": {
    "keymaster": "./cli.js"
  },
  "keymaster": {
    "envTypes": [
      "development",
      "testing",
      "production"
    ],
    "sshKeys": [
      "buildmaster",
      "centos"
    ],
    "hash": {
      "size": 32,
      "combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
    }
  },
  "pkgwrap": {
    "cleanup": [
      "cli.js",
      "cli.d.ts",
      "index.js",
      "index.d.ts",
      "test/tests.js",
      "test/tests.d.ts"
    ]
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test && yarn run report",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
