  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "empty-dir": "^0.2.1",
    "lodash": "^4.17.4",
    "log4js": "^2.3.3",
    "util.join": "0.0.14",
    "util.pkgwrap": "0.0.68",
    "util.rstrip": "0.0.26",
    "util.timestamp": "0.0.32",
    "util.toolbox": "0.0.47",
    "uuid": "^3.1.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/fs-extra": "4.0.2",
    "@types/lodash": "^4.14.74",
    "@types/log4js": "0.0.33",
    "@types/node": "^8.0.28",
    "@types/uuid": "^3.4.2",
    "@types/yargs": "^8.0.2",
    "ava": "^0.22.0",
    "coveralls": "^2.13.1",
    "fs-extra": "^4.0.1",
    "nyc": "^11.2.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "util.fixture": "0.0.56"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "keymaster": "./cli.js"
  },
  "scripts": {
    "all": "yarn install && yarn run clean && yarn run lint && yarn run build && yarn test && yarn run report",
    "build": "pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "postinstall": "pkgwrap --postinstall",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  },
  "keymaster": {
    "envTypes": [
      "development",
      "testing",
      "production"
    ],
    "sshKeys": [
      "buildmaster",
      "centos"
    ],
    "hash": {
      "size": 32,
      "combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
    }
  }
