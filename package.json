{
  "name": "util.keymaster",
  "version": "0.0.2",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Command line for generating SSH keys and self signed certs for a project.",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.keymaster"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.keymaster/issues"
  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "empty-dir": "^0.2.1",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "util.join": "0.0.7",
    "util.pkgwrap": "0.0.26",
    "util.rstrip": "0.0.16",
    "util.timestamp": "0.0.26",
    "util.toolbox": "0.0.25",
    "uuid": "^3.0.1",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "2.1.0",
    "@types/lodash": "^4.14.62",
    "@types/log4js": "0.0.32",
    "@types/node": "^7.0.13",
    "@types/uuid": "^2.0.29",
    "@types/yargs": "^6.6.0",
    "ava": "^0.19.1",
    "coveralls": "^2.13.0",
    "fs-extra": "^2.1.2",
    "nyc": "^10.2.0",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "util.fixture": "0.0.48"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "keymaster": "./cli.js"
  },
  "scripts": {
    "all": "npm install && npm run lint && npm run build && npm test && npm run report",
    "build": "./node_modules/.bin/pkgwrap --build",
    "coverage": "./node_modules/.bin/pkgwrap --coverage",
    "lint": "./node_modules/.bin/pkgwrap --lint",
    "postinstall": "./node_modules/.bin/pkgwrap --postinstall",
    "report": "./node_modules/.bin/pkgwrap --reporting",
    "test": "./node_modules/.bin/pkgwrap --testing --ava"
  },
  "keymaster": {
    "envTypes": [
      "development",
      "testing",
      "production"
    ],
    "sshKeys": [
      "buildmaster",
      "centos"
    ],
    "hash": {
      "size": 32,
      "combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
    }
  }
}
