  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.keymaster"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.keymaster/issues"
  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "empty-dir": "^0.2.1",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "util.join": "0.0.10",
    "util.pkgwrap": "0.0.43",
    "util.rstrip": "0.0.18",
    "util.timestamp": "0.0.29",
    "util.toolbox": "0.0.30",
    "uuid": "^3.0.1",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "3.0.2",
    "@types/lodash": "^4.14.64",
    "@types/log4js": "0.0.32",
    "@types/node": "^7.0.21",
    "@types/uuid": "^2.0.29",
    "@types/yargs": "^6.6.0",
    "ava": "^0.19.1",
    "coveralls": "^2.13.1",
    "fs-extra": "^3.0.1",
    "nyc": "^10.3.2",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2",
    "util.fixture": "0.0.53"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "keymaster": "./cli.js"
  },
  "scripts": {
    "all": "npm install && npm run clean && npm run lint && npm run build && npm test && npm run report",
    "build": "pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "postinstall": "pkgwrap --postinstall",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  },
  "keymaster": {
    "envTypes": [
      "development",
      "testing",
      "production"
    ],
    "sshKeys": [
      "buildmaster",
      "centos"
    ],
    "hash": {
      "size": 32,
      "combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
    }
  }
}
