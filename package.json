{
	"name": "util.keymaster",
	"version": "0.0.15",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Command line for generating SSH keys and self signed certs for a project.",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.keymaster"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.keymaster/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"main": "./index.min.js",
	"types": "./index.d.ts",
	"bin": {
		"keymaster": "./cli.js"
	},
	"dependencies": {
		"empty-dir": "^2.0.0",
		"lodash": "^4.17.11",
		"log4js": "^4.0.2",
		"util.join": "0.0.22",
		"util.rstrip": "0.0.43",
		"util.timestamp": "0.0.39",
		"util.toolbox": "0.0.69",
		"uuid": "^3.3.2",
		"yargs": "^13.2.1"
	},
	"devDependencies": {
		"@babel/core": "^7.3.3",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.3.1",
		"@types/fs-extra": "5.0.5",
		"@types/jest": "^24.0.5",
		"@types/lodash": "^4.14.121",
		"@types/log4js": "2.3.5",
		"@types/node": "^11.9.4",
		"@types/uuid": "^3.4.4",
		"@types/yargs": "^12.0.9",
		"babel-minify": "^0.5.0",
		"coveralls": "^3.0.2",
		"fs-extra": "^7.0.1",
		"jest": "^24.1.0",
		"prettier": "^1.16.4",
		"tslint": "^5.12.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.3.3",
		"util.fixture": "0.0.66",
		"util.pkgwrap": "^0.0.98"
	},
	"keymaster": {
		"envTypes": [
			"development",
			"testing",
			"production"
		],
		"sshKeys": [
			"buildmaster",
			"centos"
		],
		"hash": {
			"size": 32,
			"combo": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
		}
	},
	"pkgwrap": {
		"cleanup": [
			"cli.js",
			"cli.js.map",
			"cli.d.ts"
		]
	},
	"scripts": {
		"all": "yarn install && yarn prettier && yarn build && yarn test && yarn minify",
		"build": "yarn clean && yarn lint && pkgwrap --build",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"lint": "pkgwrap --lint",
		"minify": "minify ./index.js --out-file ./index.min.js",
		"prepublishOnly": "yarn run all",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,css}\"",
		"test": "jest"
	}
}
